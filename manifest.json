{
    "manifest_version": 2,
    "name": "tab reminder v2",
    "description": "for your productivity",
    "version": "1.1",
    "icons": {
    "19": "/icons/19man.png"
    },
    "background": {
        "scripts": ["src/js/background.js"]
    },
    "browser_action": {
        "default_sicon": {
            "19": "/icons/19man.png"
        },
        "default_title": "Tab Reminder",
        "default_popup": "/src/html/popup.html"
    },
    "permissions": [
    "tabs",
    "unlimitedStorage",
    "notifications"
    ]
}